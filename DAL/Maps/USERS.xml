      select USERID,USERNAME,LOGID,CUSID,CUSORGID,SEX,DPT,PSTN,IDNO,ADR,OTEL,ITEL,MOVTEL,EMAIL,QQ,PWD,CHKVU,USERTYP,STT,FNDPWDVLD,PIC,ISSYS from users
